<%= render 'navbar' %>

<table class="table table-striped table-bordered table-hover table-condensed table-response">
  <tr>

  <th>
    username
  </th>
  <th>
    created_at
  </th>

  <th>
    action
  </th>
  </tr>
  <% @users.map do |user| %>

    <tr>
      <td>
        <%= user.username %>
      </td>
      <td>
        <%= user.created_at %>
      </td>
      <td>
        <%= link_to 'Destroy', admins_user_path(user),  method: :delete, data: { confirm: 'Are you sure?' } %>
      </td>
    </tr>

  <% end %>
</table>

